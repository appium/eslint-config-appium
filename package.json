{
  "name": "eslint-config-appium",
  "version": "3.0.0",
  "description": "Shared config for Appium's eslint style",
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/eslint-config-appium"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint:find:current-rules": "eslint-find-rules -c",
    "eslint:find:all-rules": "eslint-find-rules -a",
    "eslint:find:plugin-rules": "eslint-find-rules -p",
    "eslint:find:unused-rules": "eslint-find-rules -u -n"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "appium",
    "es2015"
  ],
  "author": "appium",
  "license": "Apache-2.0",
  "dependencies": {},
  "peerDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-promise": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.2.0",
    "eslint-find-rules": "^3.2.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-promise": "^4.0.0"
  }
}
